# ìˆ˜ì—´

import sys

n, k = sys.stdin.readline().split()
n = int(n)
k = int(k)

nums = list(map(int, sys.stdin.readline().split()))
sums = [0] * n
sums[0] = nums[0]
for i in range(1, n):
    sums[i] = sums[i - 1] + nums[i]

ans = [0] * (n - (k - 1))

for i in range(n - (k - 1)):
    if i == 0:
        ans[i] = sums[i + (k - 1)]
    else:
        ans[i] = sums[i + (k - 1)] - sums[i - 1]

print(max(ans))
